sudo sh -c "/usr/bin/echo 3 > /proc/sys/vm/drop_caches"
echo blockSize 1 uncached time:
./run testFile1byte -r 1 33554432
echo blockSize 1 cached time:
./run testFile1byte -r 1 33554432

